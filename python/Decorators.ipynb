     "text": [
      "About to run the function...\n",
      "Hello, world!\n",
      "Done with the function.\n"
     ]
    }
   ],
   "source": [
    "def announce(f):\n",
    "  def wrapper():\n",
    "    print(\"About to run the function...\")\n",
    "    f()\n",
    "    print(\"Done with the function.\")\n",
    "  return wrapper\n",
    "\n",
    "@announce\n",
    "def hello():\n",
    "  print(\"Hello, world!\")\n",
    "\n",
    "hello()"
   ]
